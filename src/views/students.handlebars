<div>
    <h1>Lista Estudiantes</h1>
    <ul>
        {{#each data.docs}}
        <li>{{this.first_name}} {{this.last_name}}</li>
        {{/each}}
    </ul>
    <br />
    <br />
    {{#if data.hasPrevPage}}
    <button><a href={{data.prevLink}}>Pagina previa</a></button>
    {{/if}}

    {{#if data.hasNextPage}}
    <button><a href={{data.nextLink}}>Pagina Siguiente</a></button>
    {{/if}}

    <span>Pagina: {{data.page}}</span>
</div>